		        <menuitem type="checkbox" label="Enabled" oncommand="Firebug.HandcarModule.onToggleOption(this)" option="enabled" checked="true" id="HandcarEnabled"/>
		        <menuseparator/>
  				</menupopup>
        </toolbarbutton>
    </toolbar>
</overlay>
